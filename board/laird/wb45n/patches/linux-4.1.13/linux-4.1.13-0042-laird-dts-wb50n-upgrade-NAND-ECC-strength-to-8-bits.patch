From 8d6facf3ac2f515536b598bf81b396445e5f5fdf Mon Sep 17 00:00:00 2001
From: Steve deRosier <steve.derosier@lairdtech.com>
Date: Sat, 5 Dec 2015 12:55:54 -0800
Subject: [PATCH] laird: dts: wb50n: upgrade NAND ECC strength to 8 bits

Bug: 8689
---
 arch/arm/boot/dts/wb50n.dts | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm/boot/dts/wb50n.dts b/arch/arm/boot/dts/wb50n.dts
index cd663ef..f062f39 100644
--- a/arch/arm/boot/dts/wb50n.dts
+++ b/arch/arm/boot/dts/wb50n.dts
@@ -161,7 +161,7 @@
 			nand-bus-width = <8>;
 			nand-ecc-mode = "hw";
 			atmel,has-pmecc;
-			atmel,pmecc-cap = <4>;
+			atmel,pmecc-cap = <8>;
 			atmel,pmecc-sector-size = <512>;
 			nand-on-flash-bbt;
 			status = "okay";
-- 
