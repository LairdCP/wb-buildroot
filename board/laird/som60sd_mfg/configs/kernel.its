/dts-v1/;

/ {
	description = "Laird OS kernel image & FDT blob";
	#address-cells = <1>;

	images {
		kernel@1 {
			data = /incbin/("zImage");
			type = "kernel";
			arch = "arm";
			os = "linux";
			compression = "none";
			load = <0x22000000>;
			entry = <0x22000000>;
			kernel-version = <1>;
			hash@1 {
				algo = "sha256";
			};
		};
		fdt@1 {
			data = /incbin/("at91-dvk_som60sd_mfg.dtb");
			type = "flat_dt";
			arch = "arm";
			compression = "none";
			fdt-version = <1>;
			hash@1 {
				algo = "sha256";
			};
		};
		script@1 {
			data = /incbin/("boot.scr");
			type = "script";
			arch = "arm";
			compression = "none";
			hash@1 {
				algo = "sha256";
			};
		};
	};
	configurations {
		default = "config@1";
		config@1 {
			kernel = "kernel@1";
			fdt = "fdt@1";
			loadables = "script@1";
			signature@1 {
				algo = "sha256,rsa2048";
				key-name-hint = "dev";
				sign-images = "loadables", "fdt", "kernel";
			};
		};
	};
};
