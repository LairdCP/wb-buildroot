rootfs="ubi.block=0,${bootvol}"
dm_table="vroot,,ro,0 SIZE verity 1 /dev/ubiblock0_${bootvol} /dev/ubiblock0_${bootvol} 4096 4096 BLOCKS OFFSET sha256 HASH SALT"

initlrd="initlrd=/usr/sbin/pre-systemd-init.sh"

setenv bootargs "ubi.mtd=ubi ${rootfs} dm=\"${dm_table}\" root=/dev/dm-0 rootfstype=squashfs ro \
	"init=/usr/sbin/fipsInit.sh ${initlrd} " \
	"fips=${fips:=0} fips_wifi=${fips_wifi:=0} loglevel=3 systemd.show_status=0"
